{
  "extends": ["airbnb", "prettier", "plugin:node/recommended"],
  "plugins": ["prettier"],
  "root": true,
  "env": {
    "node": true 
  },
  "parserOptions": {
    "ecmaVersion": 2020
  },
  "rules": {
    // "prettier/prettier": "error",
    "no-unused-vars": "warn",
    "no-unsafe-optional-chaining": "off",
    "no-console": "off",
    "consistent-return": "off",
    "func-names": "off",
    "no-process-exit": "off",
    "object-shorthand": "off",
    "class-methods-use-this": "off"
  }
}
